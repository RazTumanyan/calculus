Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"criticalPoints", "[", "expr_", "]"}], ":=", " ", 
   RowBox[{"Module", "[", "\n", "\t", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"solutions", ",", " ", "points"}], "}"}], "\n", "\t", ",", "\n",
      "\t", 
     RowBox[{
      RowBox[{"solutions", " ", "=", " ", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"D", "[", 
           RowBox[{"expr", ",", " ", "x"}], "]"}], " ", "\[Equal]", " ", 
          "0"}], ",", " ", "x"}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"solutions", " ", "==", " ", 
         RowBox[{"{", "}"}]}], ",", "\n", "\t\t", 
        RowBox[{"Failure", "[", 
         RowBox[{"\"\<NoCriticalPoints\>\"", ",", "\n", "\t\t\t", 
          RowBox[{"<|", "\n", "\t\t\t\t", 
           RowBox[{
            RowBox[{
            "\"\<MessageTemplate\>\"", " ", "->", " ", 
             "\"\<`function` function doesn't have critical points\>\""}], 
            ",", "\n", "\t\t\t\t", 
            RowBox[{"\"\<MessageParameters\>\"", " ", "->", " ", 
             RowBox[{"<|", 
              RowBox[{"\"\<function\>\"", " ", "->", " ", "expr"}], 
              "|>"}]}]}], "\n", "\t\t\t", "|>"}]}], "\n", "\t\t", "]"}], ",", 
        "\n", "\t\t", 
        RowBox[{
         RowBox[{"points", " ", "=", " ", 
          RowBox[{"Values", "[", "solutions", "]"}]}], ";", 
         RowBox[{"(*", " ", 
          RowBox[{"Extract", " ", "solutions"}], " ", "*)"}], "\n", "\t\t", 
         RowBox[{"Style", "[", "\n", "\t\t\t", 
          RowBox[{
           RowBox[{"StringDelete", "[", 
            RowBox[{
             RowBox[{"ToString", "[", 
              RowBox[{"points", ",", " ", "InputForm"}], "]"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"\"\<{\>\"", ",", " ", "\"\<}\>\""}], "}"}]}], "]"}], 
           ",", "\n", "\t\t\t", 
